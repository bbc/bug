{
    "name": "mikrotik-interfaces",
    "main": "index.js",
    "scripts": {
        "production": "concurrently \"node ./api/server\" \"cd workers && node fetch-interfaces.js\" \"cd workers && node fetch-linkstats.js\" \"cd workers && node fetch-traffic.js\"",
        "development": "concurrently \"nodemon ./api/server.js\" \"cd workers && nodemon fetch-interfaces.js\"  \"cd workers && nodemon fetch-linkstats.js\" \"cd workers && nodemon fetch-traffic.js\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "nodemonConfig": {
        "ignore": [
            "./data/*",
            "./config/*"
        ]
    },
    "author": "Geoff House",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.21.1",
        "concurrently": "^6.0.0",
        "delay": "^5.0.0",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "http-errors": "^1.8.0",
        "io-videohub": "^2.0.0",
        "module-alias": "^2.2.2",
        "mongodb": "^3.6.6",
        "morgan": "^1.10.0",
        "nedb": "^1.8.0",
        "nedb-promises": "^4.1.1",
        "node-routeros": "^1.6.9",
        "package.json": "^2.0.1",
        "path": "^0.12.7",
        "random-words": "^1.1.1",
        "routeros-client": "^1.1.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "jest": "^26.6.0",
        "nodemon": "^2.0.7",
        "superagent": "^6.1.0",
        "supertest": "^6.1.3"
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "./node_modules/"
        ]
    },
    "_moduleAliases": {
        "@root": ".",
        "@routes": "./api/routes/",
        "@api": "./api/",
        "@data": "./data/",
        "@utils": "./utils/"
    }
